<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools" android:layout_width="match_parent"
    android:layout_height="match_parent" android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    android:paddingBottom="@dimen/activity_vertical_margin"
    tools:context="dakoto.wecycle.SuscriberHome">

    <ImageButton
        android:layout_width="match_parent"
        android:layout_height="185dp"
        android:id="@+id/imageButton3"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="41dp"
        android:background="@drawable/sched"
        android:elevation="@dimen/abc_action_bar_default_padding_material" />

    <ImageButton
        android:layout_width="match_parent"
        android:layout_height="185dp"
        android:id="@+id/imageButton4"
        android:layout_marginTop="41dp"
        android:layout_below="@+id/imageButton3"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        style="@style/Base.TextAppearance.AppCompat.Button"
        android:background="@drawable/hist"
        android:adjustViewBounds="false"
        android:clickable="true"
        android:cropToPadding="true"
        android:elevation="@dimen/abc_action_bar_default_height_material"
        android:focusable="true"
        android:focusableInTouchMode="false"
        android:padding="5dp" />
</RelativeLayout>
